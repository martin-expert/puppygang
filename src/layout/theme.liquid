<!doctype html>
<html class="no-js" lang="{{ shop.locale }}">

<head>
  <script>document.documentElement.className=document.documentElement.className.replace(/\bno-js\b/,'js');</script>
  <meta charset="utf-8">
  <meta http-equiv="x-ua-compatible" content="ie=edge">

  {% assign tab_index = 0 %}

  {% if template == 'index' %}
    <title>{{ shop.name }} | {{ page_title }}</title>
  {% elsif template == '404' %}
    <title>{{ "general.404.title" | t }} | {{ shop.name }}</title>
  {% else %}
    <title>{{ page_title }} | {{ shop.name }}</title>
  {% endif %}

  {% if page_description %}
    <meta name="description" content="{{ page_description | escape }}" />
  {% endif %}

  {% include 'favicon' %}

  {% if template contains 'collection' and current_tags %}
    <meta name="robots" content="noindex" />
    <link rel="canonical" href="{{ shop.url }}{{ collection.url }}" />
  {% else %}
    <link rel="canonical" href="{{ canonical_url }}" />
  {% endif %}

  <meta name="viewport" content="width=device-width,initial-scale=1" />

  {{ 'bootstrap.min.css' | asset_url | stylesheet_tag }}
  
  {% include 'treedify' %}{{ content_for_header }}

  {% assign twitter_handle = settings.social_twitter_url | split: 'twitter.com/' | last %}
  {%
    include 'social-meta-tags',
    twitter_handle: twitter_handle
  %}

  {% include 'scripts-styles-header' %}
  
  <link href="https://fonts.googleapis.com/css?family=Signika+Negative:400,700" rel="stylesheet">


</head>

<style>
  
  .subspop.parnone{
    display:none !important;
    opacity:0 !important;
    visibility:hidden !important;
    height:0 !important;
    width:0 !important;
  }
  </style>
  
  
{% include 'additional-classes' %}
<body class="{{ bodyClasses }}">
  
  
  <div class="loderr"></div>
  <div class="subspop">
    	<div class="subspop_inner">
          <div class="subspop_innerset">
          <a href="#" class="clspop"><img src="https://cdn.shopify.com/s/files/1/0076/5499/8063/files/cross.png?4704" alt=""></a>
          	<div class="subspop_tit">Get 20% off on your first freshly prepared doggy meal !</div>
    		<!--{% section 'static-newsletter' %}-->
          	<div class="klaviyo-form-H3PZZk"></div>
          	<div class="subspop_promo">{% section 'pxs-announcement-bar' %}</div>
          </div>
    	</div>
  </div>

  

  {% section 'pxs-announcement-bar' %}

  {% section 'static-header' %}

  <main class="main-content">
    
    

    
    
    {% unless template contains 'index' or fullBleed %}
      {% unless template contains 'slideshow' and settings.product-slideshow-layout == 'full-width' %}
        {% include 'breadcrumbs' %}
      {% endunless %}
    {% endunless %}

    {{ content_for_layout }}
	<noscript class="endOfLayoutContentX" type="text/mark"></noscript>{% include 'treedify_script' %}
    <div class="instafeeed">
    	<div id="insta-feed"></div>
    </div>
    
    
  </main>

  {% section 'static-newsletter' %}

  {% section 'static-social-feeds' %}

  {% section 'static-footer' %}

  {% include 'scripts-styles-footer' %}

  {% include 'structured-data' %}

  
  
  
<!-- Begin ReCharge code -->
{% include 'subscription-theme-footer' %}
<!-- End ReCharge code -->
  
  
  
  
  
 <script type="text/javascript" async 
 src="https://static.klaviyo.com/onsite/js/klaviyo.js?company_id=LB9v2y"></script> 
  
  
  
  <script>
  
  $('input[type="radio"').click(function() { 
        var new_href =$(this).next('a').attr('href');
        $(".button.inverse").attr("href", new_href);        
    });
  
</script>
  
  
</body>
</html>
